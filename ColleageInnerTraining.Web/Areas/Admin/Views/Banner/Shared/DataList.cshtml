@model List<ColleageInnerTraining.Application.Dtos.BannerListDto>
<table class="table_info">
    <tr>
        <th class="col-xs-2 text-center">标题</th>
        <th class="col-xs-2 text-center">简图</th>
        <th class="col-xs-1 text-center">开启时间</th>
        <th class="col-xs-1 text-center">关闭时间</th>
        <th class="col-xs-1 text-center">类型</th>
        <th class="col-xs-1 text-center">排序</th>
        <th class="col-xs-1 text-center">状态</th>
        <th class="text-center">操作</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td style="text-align:left;padding-left:5px">@item.Title</td>
            <td><img src="@item.ImageUrl" style="width:80px;height:60px"/> </td>
            <td>@item.StartTime.ToString("yyyy-MM-dd")</td>
            <td>@item.EndTime.ToString("yyyy-MM-dd")</td>
            <td>@(item.ClientType==1? "PC端" : "Wap端")</td>
            <td>@item.Sort </td>
            <td>@(item.Enabled ? "启用" : "禁用")</td>
            <td>
                <a class="yanhse" href="/Admin/Banner/Create?id=@item.Id">修改</a> |
                <a class="lanse" onclick="del(@item.Id)">删除</a>
            </td>
        </tr>
    }
</table>
@{Html.RenderPartial("~/Areas/Admin/Views/Shared/Pager.cshtml");}