@model IEnumerable<ColleageInnerTraining.Application.Dtos.ClassTrainingInfoListDto>
<table class="table_info">
    <tr>
        <th class="col-xs-2 text-center">项目名称</th>
        <th class="col-xs-2 text-center">类型</th>
        <th class="col-xs-2 text-center">开始时间</th>
        <th class="col-xs-2 text-center">结束时间</th>
        <th class="text-center">操作</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td style="text-align:left;padding-left:5px">@item.Name</td>
            <td>@(item.TrainingType == 1 ? "课程" : item.TrainingType == 2 ? "考试" : item.TrainingType == 3 ? "问卷" : "线下培训") </td>
            <td>@item.StartTime.ToString("yyyy-MM-dd")</td>
            <td>@item.EndTime.ToString("yyyy-MM-dd")</td>
            <td><a class="lanse" onclick="remove(@item.Id,@item.TrainingType)">移除</a></td>
        </tr>
    }
</table>
@{Html.RenderPartial("~/Areas/Admin/Views/Shared/Pager.cshtml");}
