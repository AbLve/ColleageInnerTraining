@using Abp.Application.Navigation
@using Abp.Collections.Extensions
@using Abp.Extensions
@using ColleageInnerTraining.Web.Views
@model ColleageInnerTraining.Web.Admin.Models.LeftViewModel
<div class="side_menu fl">
    <h3 class="f14 c66">@Model.MenuParentName</h3>
    <div class="side_nav">
        @for (int i = 0; i < Model.Menu.Count; i++)
        {
            var menu = Model.Menu[i];              
            <a class="@(Model.CurrentPageName==menu.Path ? "cur":"")" href="@menu.Url?menuId=@menu.Id">@menu.MenuName </a> 
        }

    </div>
</div>