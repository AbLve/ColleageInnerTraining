@model List<ColleageInnerTraining.Application.Dtos.NoticeListDto>
<table class="table_info">
    <tr>
        <th class="col-xs-3 text-center">标题</th>
        @*<th class="col-xs-2 text-center">发布者</th>*@
        <th class="col-xs-2 text-center">开启时间</th>
        <th class="col-xs-2 text-center">关闭时间</th>
        <th class="col-xs-1 text-center">状态</th>
        <th class="text-center">操作</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td style="text-align:left;padding-left:5px">@item.Title</td>
            @*<td>@item.CreatorUserName</td>*@
            <td>@item.StartTime.ToString("yyyy-MM-dd")</td>
            <td>@item.EndTime.ToString("yyyy-MM-dd")</td>
            <td>@(item.Enabled ? "启用" : "禁用")</td>
            <td>
                <a class="yanhse" href="/Admin/Notice/Create?id=@item.Id">修改</a> |
                <a class="lanse" onclick="del(@item.Id)">删除</a> |
                <a class="yanhse" href="/Admin/NoticeSet/Index?nId=@item.Id&nName=@item.Title">其他设置</a>
            </td>
        </tr>
    }
</table>
@{Html.RenderPartial("~/Areas/Admin/Views/Shared/Pager.cshtml");}