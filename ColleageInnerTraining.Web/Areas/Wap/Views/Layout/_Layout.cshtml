@using System.Threading
@using System.Web.Optimization
@using Abp.Timing
@using Abp.Web.Mvc.Extensions
@using ColleageInnerTraining.Web.Bundling
@{
    SetAntiForgeryCookie();
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="~/favicon.ico">
    <title>国资商学院</title>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <link rel="shortcut icon" href="~/favicon.ico" />
    <link href="~/Content/fonts-open-sans.css" rel="stylesheet" type="text/css" />
    @Styles.Render("~/Bundles/Wap/css")
    <script type="text/javascript">
        @* This is used to get the application's root path from javascript.
        It's useful if you're running application in a virtual directory under IIS. *@
        var abp = abp || {}; abp.appPath = '@ApplicationPath';
    </script>
    <!--[if lt IE 9]>
        <script src="~/libs/respond/respond.min.js"></script>
        <script src="~/libs/html5shiv/html5shiv.js"></script>
        <script src="~/libs/excanvas/excanvas.min.js"></script>
    <![endif]-->

    @Scripts.Render("~/Bundles/Wap/libs/js")
    @Scripts.Render("~/Bundles/Wap/js")
</head>
<body>
   
    <div class="Amain">
    @Html.Action("Header", "Layout", new { currentPageName = ViewBag.CurrentPageName })
    @RenderBody()
    @Html.Action("Footer", "Layout", new { currentPageName = ViewBag.CurrentPageName })
    </div>

    @RenderSection("Scripts", false)

</body>
</html>