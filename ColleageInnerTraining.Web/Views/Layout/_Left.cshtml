@using Abp.Application.Navigation
@using Abp.Collections.Extensions
@using Abp.Extensions
@using ColleageInnerTraining.Web.Views
@model ColleageInnerTraining.Web.Models.Layout.LeftViewModel
<div class="side_menu fl">
    <h3 class="f14 c66">用户管理</h3>
    <div class="side_nav">
        @for (int i = 0; i < Model.Menu.Count; i++)
        {
            var menu = Model.Menu[i];              
            <a class="@(Model.CurrentPageName==menu.Path ? "cur":"")" href="@menu.Url">@menu.MenuName</a> 
        }

    </div>
</div>